@page
@model WebOAS.Pages.fsAccounts.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].AccntNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].AccntTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].IncreaseCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].fsAccntCategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fsAccount[0].fsEntity)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.fsAccount) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AccntNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccntTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncreaseCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fsAccntCategory.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fsEntity.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
